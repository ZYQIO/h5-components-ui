{
  "name": "my-h5-components",
  "version": "1.0.0",
  "private": false,
  "description": "移动端 H5 组件库",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite -c example/vite.config.ts serve example",
    "build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly && vite build",
    "publish:yalc": "npm run build && yalc publish",
    "push:yalc": "npm run build && yalc push",
    "watch:yalc": "nodemon --watch src -e vue,ts,js,css --exec 'npm run push:yalc'",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.10",
    "typescript": "~5.0.4",
    "vant": "^4.0.0",
    "vite": "^5.0.0",
    "vue": "^3.0.0",
    "vue-tsc": "^1.8.5"
  },
  "peerDependencies": {
    "vant": "^4.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "vue": "^3.0.0"
  }
}
